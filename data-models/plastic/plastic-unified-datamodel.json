  "useCases": [
    {
      "name": "Digital Twin for Manufacturing",
      "description": "Track complete product lifecycle from raw materials through components to final assembly, including energy consumption and weight at each stage."
    },
    {
      "name": "Supply Chain Optimization",
      "description": "Use optimal-supplier entities to recommend best suppliers based on stock availability, pricing, and logistics optimization."
    },
    {
      "name": "Inventory Management",
      "description": "Monitor stock levels across materials, components, and warehouses to trigger automatic reordering when thresholds are reached."
    },
    {
      "name": "Production Planning",
      "description": "Calculate material requirements for production orders by traversing relationships from products to components to materials, factoring in current stock levels."
    },
    {
      "name": "Circular Economy Tracking",
      "description": "Track material composition and recycling potential throughout the product lifecycle to support circular economy initiatives."
    },
    {
      "name": "Energy Monitoring",
      "description": "Aggregate energy consumption data from individual components to calculate total environmental impact of manufactured products."
    }
  ],
  "workflowDescription": {
    "orderProcessing": {
      "step1": "Receive production order for N units of a ManufacturingMachine",
      "step2": "Query stockLevel of each ManufacturingComponent referenced in hasComponent relationship",
      "step3": "For components with insufficient stock, follow hasMaterial relationship to identify required raw materials",
      "step4": "Calculate total material requirements: component.weight Ã— quantity_needed",
      "step5": "Check Material.stockLevel against calculated requirements",
      "step6": "For materials with insufficient stock, query OptimalSupplier entities for sourcing recommendations",
      "step7": "Create OrderRequest entities for required materials using recommended suppliers",
      "step8": "Schedule production based on material availability and aggregate totalEnergyConsumption",
      "step9": "Update stockLevel for consumed materials and components; update lastTimeUsed timestamps"
    }
  }
}
